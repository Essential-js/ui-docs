{
	"name": "ui-docs",
	"version": "0.0.1",
	"private": "true",
	"description": "Essential UI official documentation.",
	"author": "bryantdev_",
	"license": "MIT",
	"scope": "essential-js",
	"title": "Essential UI Docs",
	"layout": "main-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"params": {
		"menu": [
			{
				"name": "Form",
				"icon": "form",
				"subComponents": [
					{
						"name": "Input",
						"path": "inputs"
					},
					{
						"name": "Textarea",
						"path": "textarea"
					},
					{
						"name": "Buttons",
						"path": "buttons"
					}
				]
			},
			{
				"name": "Utils",
				"icon": "utils",
				"subComponents": [
					{
						"name": "Modals",
						"path": "modals"
					},
					{
						"name": "Element Title",
						"path": "element-title"
					},
					{
						"name": "Toast",
						"path": "toast"
					}
				]
			},
			{
				"name": "Layout",
				"icon": "layout",
				"subComponents": [
					{
						"name": "Card",
						"path": "card"
					}
				]
			}
		]
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 3003
				}
			},
			{
				"name": "npm",
				"npm": {
					"platforms": {
						"web": "web"
					}
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/react-widgets": "^18.20.4",
		"@essential-js/ui": "^0.0.4",
		"framer-motion": "^9.0.2",
		"iconsax-react": "^0.0.8",
		"nanoid": "^4.0.0",
		"prism-react-renderer": "^1.3.5",
		"socket.io-client": "^4.5.3"
	},
	"devDependencies": {
		"@beyond-js/local": "^0.1.2"
	}
}
